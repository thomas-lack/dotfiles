{
  "telemetry.telemetryLevel": "off",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "editor.fontFamily": "'CaskaydiaMono Nerd Font', 'Cascadia Code', 'DroidSansMono Nerd Font', 'monospace', monospace",
  "editor.minimap.enabled": false,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": true,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.surround": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["1", "0", "j", "z", "z"]
    },
    {
      "before": ["K"],
      "after": ["1", "0", "k", "z", "z"]
    },
    {
      "before": [";"],
      "after": ["A", ";", "<Esc>"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    {
      "before": ["0"],
      "after": ["^"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "R"],
      "commands": ["editor.action.refactor"]
    },
    {
      "before": ["g", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["filesExplorer.findInFolder"]
    },
    {
      "before": ["<Enter>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "E"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.splitEditorOrthogonal"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["1", "0", "j", "z", "z"]
    },
    {
      "before": ["K"],
      "after": ["1", "0", "k", "z", "z"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.blockComment"]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "explorer.confirmDelete": true,
  "playwright.reuseBrowser": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "breadcrumbs.enabled": false,
  "editor.stickyScroll.enabled": false,
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "git.autofetch": true,
  "playwright.showTrace": false,
  "rust-analyzer.rustfmt.enableRangeFormatting": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.codeLens": false
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "tabby.endpoint": "http://localhost:7488",
  "tabby.config.telemetry": false
}
